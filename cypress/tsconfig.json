{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "es5",
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "types": [
      "cypress",
      "@testing-library/cypress",
      "@bahmutov/cy-grep",
      "cypress-plugin-api",
      "cypress-real-events",
      "diff",
      "fs-extra",
      "adm-zip"
    ],
    "strict": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowJs": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "isolatedModules": false,
    "noEmit": true,
    "jsx": "react-jsx"
  },
  "include": [
    "**/*.ts",
    "**/*.js"
  ],
}
